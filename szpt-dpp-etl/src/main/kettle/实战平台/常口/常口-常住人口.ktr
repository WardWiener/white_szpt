        <class_source><![CDATA[
import java.util.HashMap;
import java.util.Map;
import com.taiji.pubsec.szpt.solr.SolrHelper;
import java.text.SimpleDateFormat;
import org.apache.commons.lang3.StringUtils;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws Exception
{
	if (first){
	  first = false;

	  /* TODO: Your code here. (Using info fields)

	  FieldHelper infoField = get(Fields.Info, "info_field_name");

	  RowSet infoStream = findInfoRowSet("info_stream_tag");

	  Object[] infoRow = null;

	  int infoRowCount = 0;

	  // Read all rows from info step before calling getRow() method, which returns first row from any
	  // input rowset. As rowMeta for info and input steps varies getRow() can lead to errors.
	  while((infoRow = getRowFrom(infoStream)) != null){

	    // do something with info data
	    infoRowCount++;
	  }
	  */
	}

	Object[] r = getRow();

	if (r == null) {
	  setOutputDone();
	  return false;
	}

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    r = createOutputRow(r, data.outputRowMeta.size());
	
    /* TODO: Your code here. (See Sample)
    
    // Get the value from an input field
    String foobar = get(Fields.In, "a_fieldname").getString(r);

    foobar += "bar";
    
    // Set a value in a new output field
    get(Fields.Out, "output_fieldname").setValue(r, foobar);

	*/
	
	
	System.setProperty("solr.zkhost",get(Fields.In, "system_property").getString(r));

	SimpleDateFormat sdfdst = new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss'Z'");
	SimpleDateFormat sdfsrc_collecttime = new SimpleDateFormat("yyyy/MM/dd HH:mm:ss.SSS");
	SimpleDateFormat sdfsrc_birth = new SimpleDateFormat("yyyy/MM/dd");

	String idcard = get(Fields.In, "PID").getString(r);
	String name = get(Fields.In, "NAME").getString(r);
	String oldname = get(Fields.In, "USED_NAME").getString(r);
	String gender = get(Fields.In, "GENDER").getString(r);

	String collecttime_f = null;
	String collecttime = get(Fields.In, "WHEN_IN_LOGGED").getString(r);
	if(collecttime!=null){
		collecttime_f = sdfdst.format( sdfsrc_collecttime.parse(collecttime) );
	}

	String nation = get(Fields.In, "NATION_ZW").getString(r);

	String birthday_f = null;
	String birthday = get(Fields.In, "DOB").getString(r);
	if(birthday!=null){
		birthday_f = sdfdst.format( sdfsrc_birth.parse(birthday) );
	}

	String birthaddress = get(Fields.In, "NATAL_PLACE_ZW").getString(r);
	String culture = get(Fields.In, "EDU_DEGREE_ZW").getString(r);
	String marry = get(Fields.In, "MARRIAGE").getString(r);
	String occupation = get(Fields.In, "JOB").getString(r);
	String phone = get(Fields.In, "SCONTACT").getString(r);
	String relation = get(Fields.In, "MASTER_RELATION").getString(r);


	String persontypecode = get(Fields.In, "gwrylx_code").getString(r);
	String criminaltypecode = get(Fields.In, "qklx_code").getString(r);
	List  persontypecodeList = new ArrayList();
	List  criminaltypecodeList = new ArrayList();
	if(StringUtils.isNotBlank(persontypecode)){
		String[] arr = persontypecode.split(",");
		persontypecodeList =  Arrays.asList(arr);
	}

	if(StringUtils.isNotBlank(criminaltypecode)){
		String[] arr = criminaltypecode.split(",");
		criminaltypecodeList =  Arrays.asList(arr);
	}
	
	Map map = new HashMap();
	map.put("idcard",idcard);
	map.put("name",name);
	map.put("oldname",oldname);
	map.put("gender",gender);
	map.put("collecttime",collecttime_f);
	map.put("nation",nation);
	map.put("culture",culture);
	map.put("marry",marry);
	map.put("occupation",occupation);
	map.put("relation",relation);
	map.put("phone",phone);	
	map.put("id",idcard);
	map.put("birthday",birthday_f);
	map.put("birthaddress",birthaddress);
	map.put("type",get(Fields.In, "type").getString(r));
	map.put("persontypecode",persontypecodeList);	//人员类型
	map.put("criminaltypecode",criminaltypecodeList);	//刑事前科
	map.put("address",get(Fields.In, "full_addr").getString(r));
	map.put("householder",get(Fields.In, "HU_MASTER_NAME").getString(r));
	map.put("alertlevel",get(Fields.In, "yjlx_name").getString(r));
	SolrHelper solr = SolrHelper.getInstance();
	solr.addIndex("population", "id",map);



	// Send the row on to the next step.
    putRow(data.outputRowMeta, r);

	return true;
}
